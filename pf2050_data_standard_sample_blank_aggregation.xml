<?xml version="1.0"?>
<pf2050_data_standard xmlns="https://pf2050.co.nz" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://pf2050.co.nz file:///C:/Dev/data-standard/pf2050_data_standard.xsd">
	<system>
        <system_id></system_id>
        <name></name>
        <data_standard_version></data_standard_version>
    </system>
    <reported_by>
        <name></name>
    </reported_by>
    <!-- report element can appear many times in the document -->
    <report>
		<report_id></report_id>
		<report_type></report_type>
		<date_recorded>
            <date_start></date_start>
            <date_end></date_end>
		</date_recorded>
		<location></location>
		<device>
            <device_id></device_id>
			<device_type></device_type>
			<device_model></device_model>
			<device_statuses>
				<arrival></arrival>
                <departure></departure>
                <trigger_count></trigger_count>
			</device_statuses>
			<lure>
                <lure_id></lure_id>
                <lure_type></lure_type>
                <lure_statuses>
					<arrival></arrival>
                    <departure></departure>
                    <amount>
                        <grams_remaining></grams_remaining>
                        <grams_taken></grams_taken>
                        <grams_added></grams_added>
                        <grams_removed></grams_removed>
                    </amount>
				</lure_statuses>
			</lure>
			<bait>
                <bait_id></bait_id>
				<bait_type></bait_type>
				<bait_statuses>
					<arrival></arrival>
                    <departure></departure>
                    <amount>
                        <grams_remaining></grams_remaining>
                        <grams_taken></grams_taken>
                        <grams_added></grams_added>
                        <grams_removed></grams_removed>
                    </amount>
				</bait_statuses>
			</bait>
		</device>
		<species>
            <species_id></species_id>
            <common_name></common_name>
            <scientific_name></scientific_name>
            <age></age>
            <sex></sex>
            <count></count>
            <target></target>
        </species>
		<reported_by>
            <name></name>
        </reported_by>
    </report>
    <report>
        <!-- ... -->
    </report>
    <report>
        <!-- ... -->
    </report>
</pf2050_data_standard>