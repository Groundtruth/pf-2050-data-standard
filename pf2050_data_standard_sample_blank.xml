<?xml version="1.0"?>
<pf2050_data_standard xmlns="https://pf2050.co.nz" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://pf2050.co.nz file:///C:/Dev/data-standard/pf2050_data_standard.xsd">
	<system>
        <system_uuid></system_uuid>
        <name></name>
        <data_standard_version></data_standard_version>
    </system>
    <reported_by>
        <reported_uuid></reported_uuid>
        <name></name>
        <project_name></project_name>
        <website_url></website_url>
        <contact_name></contact_name>
        <contact_phone></contact_phone>
        <contact_email></contact_email>
    </reported_by>
    <!-- report element can appear many times in the document -->
    <report>
		<report_uuid></report_uuid>
		<report_type></report_type>
		<date_recorded>
            <date></date>
		</date_recorded>
        <location></location>
        <!-- device element can appear many times -->
		<device>
            <device_uuid></device_uuid>
			<device_type></device_type>
            <device_model></device_model>
            <!-- device_status element can appear many times -->
			<device_status>
				<status_type></status_type><statue_term></statue_term>
            </device_status>
            <device_status>
				<status_type></status_type><statue_value></statue_value>
            </device_status>
            <device_status>
                <!-- ... -->
            </device_status>
            <!-- substance element can appear many times -->
			<substance>
                <substance_type></substance_type>
                <substance_id></substance_id>
                <substance_name></substance_name>
                <!-- substance_status element can appear many times -->
                <substance_status>
                    <status_type></status_type><status_term></status_term>
                </substance_status>
                <substance_status>
                    <status_type></status_type><status_value></status_value>
                </substance_status>
                <substance_status>
                    <!-- ... -->
                </substance_status>
            </substance>
            <substance>
                <!-- ... -->
            </substance>
        </device>
        <device>
            <!-- -->
        </device>
        <!-- species element can appear many times -->
        <species>
            <species_id></species_id>
            <common_name></common_name>
            <scientific_name></scientific_name>
            <age></age>
            <sex></sex>
            <count></count>
            <target></target>
        </species>
        <species>
            <!-- ... -->
        </species>
        <reported_by>
            <reported_uuid></reported_uuid>
            <name></name>
        </reported_by>
    </report>
    <report>
        <!-- ... -->
    </report>
    <report>
        <!-- ... -->
    </report>
</pf2050_data_standard>